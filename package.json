{
  "name": "testgenie",
  "version": "0.1.0",
  "description": "AI-powered test case generator for your codebase",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "testgenie": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "testing",
    "ai",
    "cli",
    "test-generation",
    "jest",
    "vitest"
  ],
  "author": "LouayHouimli",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "^1.0.8",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.26.2",
    "@babel/types": "^7.26.0",
    "ai": "^4.0.12",
    "consola": "^3.2.3",
    "dotenv": "^16.4.7",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "simple-git": "^3.27.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.32",
    "@types/fs-extra": "^11.0.4",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.4",
    "typescript": "^5.3.3",
    "bun-types": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
