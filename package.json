{
  "name": "testgenie-ai",
  "version": "1.0.4",
  "description": "AI-powered test generator with framework support, pattern detection, and React Testing Library integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "testgenie": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testgenie-ai/testgenie.git"
  },
  "homepage": "https://github.com/testgenie-ai/testgenie#readme",
  "bugs": {
    "url": "https://github.com/testgenie-ai/testgenie/issues"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify",
    "start": "node dist/index.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "testgenie": "bun run dev",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "npm publish --access public"
  },
  "keywords": [
    "testing",
    "ai",
    "cli",
    "test-generation",
    "jest",
    "vitest",
    "mocha",
    "react-testing-library",
    "code-generation",
    "bdd",
    "tdd",
    "git-integration",
    "ast-parsing",
    "gemini-ai"
  ],
  "author": "LouayHouimli",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.0.8",
    "@ai-sdk/openai": "^1.3.22",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.26.2",
    "@babel/types": "^7.26.0",
    "@clack/prompts": "^0.11.0",
    "ai": "^4.0.12",
    "consola": "^3.2.3",
    "dotenv": "^16.4.7",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "simple-git": "^3.27.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@jest/globals": "^30.0.0-beta.3",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/yargs": "^17.0.32",
    "bun-types": "^1.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
